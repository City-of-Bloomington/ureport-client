<section id="errorMessages" class="alert alert-danger" role="alert">
    <header>
        <h1>{{ _('error') }}</h1>
    </header>
    {% for e in errorMessages %}
    <p>{{ _(e, 'errors') }}</p>
    {% endfor %}
</section>
